{
  "name": "@nebula.js/sn-scatter-plot",
  "version": "2.0.0",
  "description": "A scatter plot.",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust",
    "scatter",
    "scatterplot"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-scatter-plot.git"
  },
  "files": [
    "api-specifications",
    "dist",
    "core",
    "sn-scatter-plot-ext"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-scatter-plot.js",
  "scripts": {
    "build": "node ./scripts/build.js --core --ext",
    "build:dev": "node ./scripts/build.js --core --ext --mode development",
    "build:watch": "node ./scripts/build.js --ext --mode development -w",
    "copy:ext": "node ./scripts/copy-ext.js",
    "lint": "eslint src",
    "start": "nebula serve --type scatterplot",
    "sense": "nebula sense",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js && scriptappy-from-jsdoc -c ./spec-configs/plugins.conf.js",
    "test:unit": "aw -c aw.config.js",
    "test:rendering": "node ./scripts/run-rendering-tests.js",
    "test:rendering:local": "aw puppet --glob 'test/**/*.spec.js' --mocha.bail false --mocha.timeout 10000",
    "prepublishOnly": "rm -rf dist && yarn run build",
    "version": "yarn spec && git add api-specifications"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@material-ui/core": "4.12.3",
    "@material-ui/styles": "4.11.4",
    "@nebula.js/cli": "1.7.0",
    "@nebula.js/cli-build": "1.7.0",
    "@nebula.js/cli-sense": "1.7.0",
    "@nebula.js/cli-serve": "2.2.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "d3-color": "1.4.1",
    "d3-scale": "2.2.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "extend": "3.0.2",
    "fs-extra": "10.0.0",
    "hammerjs": "2.0.8",
    "husky": "4.3.8",
    "lint-staged": "12.0.3",
    "mocha-junit-reporter": "2.0.2",
    "picasso-plugin-hammer": "1.4.2",
    "picasso-plugin-q": "1.4.2",
    "picasso.js": "1.4.2",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.2",
    "prop-types": "^15.7.2",
    "puppeteer": "2.1.1",
    "qlik-chart-modules": "0.18.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scriptappy-from-jsdoc": "0.7.0",
    "yargs": "17.2.1"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=2.2.0"
  }
}
